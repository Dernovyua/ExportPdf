<Window x:Class="ExportDevExpress.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ExportDevExpress"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" WindowStartupLocation="CenterScreen">
    <Grid>
        <Button x:Name="GeneratePdf" Content="GeneratePdf" HorizontalAlignment="Left" Margin="312,63,0,0" VerticalAlignment="Top" Height="61" Width="122" Click="button_Click"/>
        <Button x:Name="button1" Content="OpenPreview" HorizontalAlignment="Left" Margin="312,142,0,0" VerticalAlignment="Top" Height="61" Width="122" Click="button1_Click"/>
        <Button x:Name="button2" Content="SaveDocument" HorizontalAlignment="Left" Margin="312,217,0,0" VerticalAlignment="Top" Width="122" Height="61" Click="button2_Click"/>

    </Grid>
</Window>
